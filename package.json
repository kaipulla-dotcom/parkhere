    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "@material-ui/utils": "^4.11.2",
    "axios": "^0.18.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "connect-mongo": "^3.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^5.3.16",
    "nodemon": "^1.19.4",
    "passport": "^0.4.1",
